{
  "name": "edgedriver",
  "version": "6.2.0",
  "description": "Microsofts' EdgeDriver for Node.js",
  "homepage": "https://webdriver.io",
  "license": "MIT",
  "author": {
    "name": "Barrett Sonntag",
    "email": "barrett@sosuke.com",
    "url": "https://www.sosuke.com"
  },
  "keywords": [
    "edgedriver",
    "edgedriverserver",
    "webdriver"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/webdriverio-community/node-edgedriver.git"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": [
      {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/cjs/index.js"
      },
      "./dist/cjs/index.js"
    ]
  },
  "bin": {
    "edgedriver": "./bin/edgedriver.js"
  },
  "scripts": {
    "build": "tsc -b .",
    "clean": "rimraf ./node_modules package-lock.yaml ./dist",
    "prepare": "husky",
    "install": "test -f ./dist/install.js && node ./dist/install.js || echo \"Skipping install, project not build!\"",
    "test": "run-s test:*",
    "test:lint": "eslint",
    "test:unit": "vitest --run",
    "test:e2e": "node --loader ts-node/esm ./tests/test.e2e.ts",
    "watch": "npm run build -- --watch"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.12",
    "@wdio/eslint": "^0.1.2",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "npm-run-all2": "^8.0.4",
    "release-it": "^19.0.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.12",
    "wait-port": "^1.1.0",
    "webdriverio": "^9.20.1"
  },
  "dependencies": {
    "@wdio/logger": "^9.18.0",
    "@zip.js/zip.js": "^2.8.11",
    "decamelize": "^6.0.1",
    "edge-paths": "^3.0.5",
    "fast-xml-parser": "^5.3.2",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "which": "^6.0.0"
  }
}
